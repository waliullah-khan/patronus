{
  "version": 2,
  "builds": [
    {
      "src": "public",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dashboard_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}